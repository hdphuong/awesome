                    <style>
          .clients__testimony {
            height: 475px;
          }

          .clients__testimonial_button {
            height: 8px;
            width: 8px;
            margin: 8px;
            display: inline-block;
            color: #fff;
            background-color: #fff;
            -webkit-border-radius: 4px;
            -moz-border-radius: 4px;
            border-radius: 4px;
            cursor: pointer;
          }

          .clients__testimonial_button.active {
            background-color: #FFA300;
          }
          </style>

          <script>
          var intCTimer;
          var intCSpeed = 10000;
          var intCCur = 1;
          var intCMax;
          var arrTestimonialsList;
          car_init();
          var x0; //mouse move

          function car_init() {
            intCTimer = setTimeout(car_updateSlide, intCSpeed);
            arrTestimonialsList = document.getElementById("testimonial_list").children;
            intCMax = document.getElementById("testimonial_list").childElementCount;
            for (i = 0; i < intCMax; i++) {
              var ctl = document.createElement("a");
              ctl.id = "ctl_" + i;
              ctl.attributes.href = "#";
              ctl.className = "clients__testimonial_button";
              if (i == 0) {
                ctl.className = "clients__testimonial_button active";
              }
              ctl.dataset.testimonial = i;
              ctl.onclick = function(e) {
                car_jumpto(this);
              }
              document.getElementById("carousel_control").appendChild(ctl);
            }
            document.getElementById("clients__testimony").addEventListener('mouseenter', e => {
              clearTimeout(intCTimer);
              console.log("Mouse Enter");
            });
            document.getElementById("clients__testimony").addEventListener('touchstart', e => {
              clearTimeout(intCTimer);
              x0 = unify(e).clientX;
              console.log("touchstart");
            });
            document.getElementById("clients__testimony").addEventListener('mouseleave', e => {
              clearTimeout(intCTimer);
              intCTimer = setTimeout(car_updateSlide, intCSpeed);
            });
            document.getElementById("clients__testimony").addEventListener('touchend', e => {
              clearTimeout(intCTimer);

              touch = e.changedTouches[0];
              dx = touch.pageX - x0;
              intCCur -= 1;
              lb = false;
              if (dx > 50) {
                intCCur -= 1;
                if (intCCur < 0) {
                  intCCur = intCMax - 1;
                }
                lb = true;
              }
              if (dx < 0) {
                intCCur += 1;
                if (intCCur >= intCMax) {
                  intCCur = 0;
                }
              }

              car_updateSlide(true, lb);

              intCTimer = setTimeout(car_updateSlide, intCSpeed);
            });
            document.getElementById("clients__testimony").addEventListener('touchmove', e => {
              e.preventDefault()
            }, false);
          }

          function car_updateSlide(forced = false, loopback = false) {
            //update content
            document.getElementById("clients__testimonial").innerHTML = arrTestimonialsList[intCCur].innerHTML;
            //update carousel_control
            c = document.getElementById("carousel_control").children;
            for (i = 0; i < c.length; i++) {
              c[i].className = "clients__testimonial_button";
              if (i == intCCur) {
                c[i].className = "clients__testimonial_button active";
              }
            }
            //increment intCCur
            intCCur++;
            if (intCCur >= intCMax && !loopback) {
              intCCur = 0;
            }
            //Reset timeout
            clearTimeout(intCTimer);
            if (!forced) {
              intCTimer = setTimeout(car_updateSlide, intCSpeed);
            }
          }

          function car_jumpto(b) {

            clearTimeout(intCTimer);
            //set slide
            intCCur = b.dataset.testimonial;
            car_updateSlide(true);
          }

          function unify(e) {
            return e.changedTouches ? e.changedTouches[0] : e
          };
          </script>

