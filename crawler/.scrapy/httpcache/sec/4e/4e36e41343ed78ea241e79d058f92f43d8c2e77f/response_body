    <script type="text/javascript">
        var userTrackingData = null;
        var contextData = {};
        if (typeof(tetris_pageview_contextData_PropContextDimension1) !== 'undefined') {
            contextData.PropContextDimension1 = tetris_pageview_contextData_PropContextDimension1;
        }
        contextData.PropApplicationLanguage = "en";
        ttt.init('production', { host: 'stan.new-work.se', fetchTrackingData: true });
        ttt.pageview('wbm/NewWork_Corp', 'wbm/NewWork_corp/investor_relations/corporate_governance', userTrackingData, contextData);

        function ttt_event_proptracker(value, additional) {
            var eventData = {PropTrackAction: value};
            if (typeof(additional) !== 'undefined') {
                eventData.PropContextDimension3 = additional;
            }
            ttt.event('PropTrackAction', eventData);
        }

        function ttt_event_proptracker_pdf(url) {
            console.log('track pdf: ' +  url);
            var eventData = {PropTrackAction: 'PropDownloadLinkUrl'};
            eventData.PropDownloadLinkUrl = url;
            ttt.event('PropTrackAction', eventData);
        }

